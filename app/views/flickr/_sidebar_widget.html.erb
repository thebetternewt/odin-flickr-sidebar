<div class="flickr">
	<ul>
		<% photos.each do |row| %>
		
			<% row.each do |p| %>
			<% p_info = flickr.photos.getInfo(photo_id: p.id) %>
			<li class="col-xs-6">
				<%= link_to(image_tag(FlickRaw.url_s(p_info), class: 'flickr-photo img-responsive', title: p.title, border: 0, size: '75x75'), FlickRaw.url_photopage(p_info)) %>
			</li>
			<% end %>
		</li>
		<% end %>
	</ul>
</div>